{
  "name": "resolve",
  "version": "0.17.4",
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-do-expressions": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-bind": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.6.0",
    "regenerator-runtime": "0.12.1",
    "eslint": "^5.5.0",
    "eslint-config-react-app": "^3.0.0-next.a671462c",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-spellcheck": "^0.0.10",
    "oao": "^1.5.1",
    "prettier": "^1.14.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "2.6.2",
    "cross-env": "5.2.0",
    "glob": "7.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/core/*",
      "packages/modules/*",
      "packages/adapters/bus-adapters/*",
      "packages/adapters/storage-adapters/*",
      "packages/adapters/readmodel-adapters/*",
      "packages/adapters/snapshot-adapters/*",
      "packages/adapters/subscribe-adapters/*",
      "packages/adapters/api-handler-adapters/*",
      "examples/shopping-list-advanced/*",
      "examples/*",
      "packages/modules/*",
      "tests"
    ]
  },
  "scripts": {
    "clean": "oao all \"npx rimraf dist lib es\" --parallel && npx rimraf yarn-error.log && npx oao clean",
    "lint": "eslint .",
    "prepare": "node ./babel.compile.js --run",
    "prettier": "prettier --write \"**/**.{js,json}\"",
    "publish": "oao publish -i \"examples/*\" --no-git-commit --no-changelog",
    "test": "oao all \"yarn test --color 2>&1\"",
    "test:functional": "oao run-script test:functional",
    "test:clean-snapshots": "npx oao all \"yarn test -u --color 2>&1\""
  }
}
