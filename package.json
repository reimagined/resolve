{
  "name": "resolve",
  "version": "0.19.8",
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.1.0",
    "eslint": "^5.13.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-spellcheck": "^0.0.11",
    "oao": "^1.5.1",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "glob": "^7.1.3",
    "@internal/helpers": "0.19.8"
  },
  "workspaces": {
    "packages": [
      "packages/core/*",
      "packages/modules/*",
      "packages/internal/*",
      "packages/adapters/bus-adapters/*",
      "packages/adapters/storage-adapters/*",
      "packages/adapters/readmodel-adapters/*",
      "packages/adapters/snapshot-adapters/*",
      "packages/adapters/subscribe-adapters/*",
      "packages/adapters/api-handler-adapters/*",
      "examples/*",
      "examples/shopping-list-tutorial/*",
      "packages/modules/*",
      "tests",
      "website"
    ]
  },
  "scripts": {
    "clean": "npm run clean:babel && npm run clean:packages && npx rimraf yarn-error.log && npx oao clean",
    "clean:babel": "npx oao all \"npx rimraf dist lib es\" --parallel",
    "clean:packages": "npx rimraf .packages",
    "lint": "eslint .",
    "prepare": "babel-compile",
    "build-packages": "build-packages",
    "local-registry": "local-registry",
    "prettier": "prettier --write \"**/**.{js,json,babelrc}\"",
    "publish": "npx oao publish -i \"examples/*\" --no-git-commit --no-changelog",
    "test": "npx oao all -i \"examples/shopping-list-advanced\" \"yarn test --color 2>&1\"",
    "test:functional": "npx oao run-script -i \"{examples/shopping-list-advanced,examples/shopping-list-tutorial/*}\" test:functional",
    "test:integrational": "npx oao run-script test:integrational",
    "tutorial-test:functional": "npx oao run-script -i \"examples/*\" test:functional",
    "test:clean-snapshots": "npx oao all -i \"examples/shopping-list-advanced\"  \"yarn test -u --color 2>&1\""
  }
}
