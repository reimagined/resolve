    "react-helmet": "5.2.0",
    "redux": "4.0.1",
    "resolve-redux": "0.22.10",
    "resolve-scripts": "0.22.10",
    "resolve-storage-postgresql-serverless": "0.22.10",
    "resolve-storage-lite": "0.22.10",
    "resolve-readmodel-lite": "0.22.10",
    "resolve-readmodel-postgresql-serverless": "0.22.10",
    "resolve-subscribe-mqtt": "0.22.10",
