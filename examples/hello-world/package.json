{
  "name": "hello-world",
  "version": "0.28.1-alpha",
  "private": true,
  "description": "used as a template for new reSolve applications",
  "scripts": {
    "build": "babel-node ./run.js build",
    "cloud": "babel-node ./run.js cloud",
    "dev": "babel-node ./run.js dev",
    "export-event-store": "babel-node ./run.js export-event-store",
    "import-event-store": "babel-node ./run.js import-event-store",
    "start": "babel-node ./run.js start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose --passWithNoTests",
    "test:e2e": "babel-node ./run.js test:e2e"
  },
  "dependencies": {
    "@babel/node": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@resolve-js/eventstore-lite": "0.28.1-alpha",
    "@resolve-js/eventstore-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/react-hooks": "0.28.1-alpha",
    "@resolve-js/readmodel-lite": "0.28.1-alpha",
    "@resolve-js/readmodel-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/scripts": "0.28.1-alpha",
    "react": "16.13.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "aws-sdk": "2.631.0",
    "aws-xray-sdk-core": "2.5.0",
    "babel-jest": "26.0.1",
    "jest": "26.6.3",
    "react-test-renderer": "16.13.1",
    "resolve-cloud": "*",
    "testcafe": "^1.9.1"
  }
