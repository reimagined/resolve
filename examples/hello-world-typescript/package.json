{
  "name": "hello-world-typescript",
  "version": "0.22.10",
  "description": "used as a template for new reSolve applications",
  "private": true,
  "scripts": {
    "type-check": "tsc",
    "build": "tsc && babel-node --extensions='.ts,.tsx' run.ts build",
    "dev": "tsc && babel-node --extensions='.ts,.tsx' run.ts dev",
    "start": "tsc && babel-node --extensions='.ts,.tsx' run.ts start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose",
    "test:functional": "tsc && babel-node --extensions='.ts,.tsx' run.ts test:functional"
  },
  "dependencies": {
    "babel-loader": "^8.0.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@types/node": "^12.7.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.9.5",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/reactstrap": "^8.4.2",
    "react": "^16.9.0",
    "reactstrap": "^8.1.1",
    "react-helmet": "6.0.0-beta",
    "react-dom": "^16.9.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "resolve-redux": "0.22.10",
    "resolve-command": "0.22.10",
    "resolve-scripts": "0.22.10",
    "resolve-query": "0.22.10",
    "resolve-readmodel-lite": "0.22.10",
    "resolve-readmodel-postgresql-serverless": "0.22.10",
    "resolve-scheduler-local": "0.22.10",
    "resolve-scheduler-cloud": "0.22.10",
    "resolve-storage-lite": "0.22.10",
    "resolve-storage-postgresql-serverless": "0.22.10",
    "resolve-subscribe-mqtt": "0.22.10",
    "resolve-subscribe-socket.io": "0.22.10",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.9.0",
    "testcafe": "0.23.3",
    "resolve-cloud": "*"
  }
}
