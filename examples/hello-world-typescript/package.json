{
  "name": "hello-world-typescript",
  "version": "0.28.1-alpha",
  "private": true,
  "description": "used as a template for new reSolve applications",
  "scripts": {
    "build": "tsc && babel-node --extensions=.ts,.tsx run.ts build",
    "dev": "tsc && babel-node --extensions=.ts,.tsx run.ts dev",
    "start": "tsc && babel-node --extensions=.ts,.tsx run.ts start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose --passWithNoTests",
    "test:e2e": "tsc && babel-node --extensions=.ts,.tsx run.ts test:e2e",
    "type-check": "tsc"
  },
  "dependencies": {
    "@babel/node": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-typescript": "7.9.0",
    "@resolve-js/eventstore-lite": "0.28.1-alpha",
    "@resolve-js/eventstore-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/react-hooks": "0.28.1-alpha",
    "@resolve-js/readmodel-base": "0.28.1-alpha",
    "@resolve-js/readmodel-lite": "0.28.1-alpha",
    "@resolve-js/readmodel-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/scripts": "0.28.1-alpha",
    "babel-loader": "8.1.0",
    "react": "16.13.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@types/node": "^12.7.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "26.0.1",
    "jest": "26.6.3",
    "react-test-renderer": "16.13.1",
    "resolve-cloud": "*",
    "testcafe": "^1.9.1"
  }
}
