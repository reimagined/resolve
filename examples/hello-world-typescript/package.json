{
  "name": "hello-world-typescript",
  "version": "0.24.15",
  "description": "used as a template for new reSolve applications",
  "private": true,
  "scripts": {
    "type-check": "tsc",
    "build": "tsc && babel-node --extensions=.ts,.tsx run.ts build",
    "dev": "tsc && babel-node --extensions=.ts,.tsx run.ts dev",
    "start": "tsc && babel-node --extensions=.ts,.tsx run.ts start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose",
    "test:e2e": "tsc && babel-node --extensions=.ts,.tsx run.ts test:e2e"
  },
  "dependencies": {
    "babel-loader": "8.1.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@types/node": "^12.7.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.9.5",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/reactstrap": "^8.4.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "reactstrap": "^8.1.1",
    "react-helmet": "6.0.0",
    "redux": "4.0.5",
    "react-redux": "7.2.0",
    "redux-saga": "1.0.5",
    "redux-devtools-extension": "^2.13.8",
    "resolve-redux": "0.24.15",
    "resolve-command": "0.24.15",
    "resolve-scripts": "0.24.15",
    "resolve-query": "0.24.15",
    "resolve-readmodel-base": "0.24.15",
    "resolve-readmodel-lite": "0.24.15",
    "resolve-readmodel-postgresql-serverless": "0.24.15",
    "resolve-scheduler-local": "0.24.15",
    "resolve-scheduler-cloud": "0.24.15",
    "resolve-eventstore-lite": "0.24.15",
    "resolve-eventstore-postgresql-serverless": "0.24.15",
    "resolve-subscribe-mqtt": "0.24.15",
    "resolve-subscribe-socket.io": "0.24.15"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-jest": "26.0.1",
    "jest": "26.0.1",
    "react-test-renderer": "16.13.0",
    "testcafe": "1.8.5",
    "resolve-cloud": "*"
  }
}
