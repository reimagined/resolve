{
  "name": "hello-world-typescript",
  "version": "0.22.6",
  "description": "used as a template for new reSolve applications",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "build": "tsc && node ./build/run.js build",
    "dev": "tsc && node ./build/run.js dev",
    "start": "tsc && node ./build/run.js start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose",
    "test:functional": "tsc && babel-node ./build/run.js test:functional"
  },
  "dependencies": {
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@types/node": "^12.7.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.9.5",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/reactstrap": "^8.4.2",
    "react": "^16.9.0",
    "reactstrap": "^8.1.1",
    "react-helmet": "6.0.0-beta",
    "react-dom": "^16.9.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "resolve-redux": "0.22.6",
    "resolve-command": "0.22.6",
    "resolve-scripts": "0.22.6",
    "resolve-query": "0.22.6",
    "resolve-readmodel-lite": "0.22.6",
    "resolve-readmodel-postgresql-serverless": "0.22.6",
    "resolve-scheduler-local": "0.22.6",
    "resolve-scheduler-cloud": "0.22.6",
    "resolve-storage-lite": "0.22.6",
    "resolve-storage-postgresql-serverless": "0.22.6",
    "resolve-subscribe-mqtt": "0.22.6",
    "resolve-subscribe-socket.io": "0.22.6",
    "typescript": "^3.3.4000"
