    "resolve-storage-dynamo": "0.18.2",
    "resolve-storage-lite": "0.18.2",
    "resolve-readmodel-memory": "0.18.2",
    "resolve-readmodel-mysql": "0.18.2",
    "resolve-subscribe-mqtt": "0.18.2",
    "resolve-subscribe-socket.io": "0.18.2",
    "resolve-module-auth": "0.18.2"
  },
  "devDependencies": {
