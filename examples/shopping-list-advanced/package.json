{
  "name": "shopping-list-advanced",
  "description": "shows how to work with React-Native",
  "version": "0.17.4",
  "private": true,
  "scripts": {
    "prepare": "cd scripts && node ./install.js",
    "patch": "cd scripts && node ./patch-package-jsons.js",
    "babelify": "cd scripts && node babel.compile.js --run",
    "babelify:watch": "cd scripts && node babel.compile.js --watch",
    "build": "cd scripts && node ./run.js build",
    "dev": "cd scripts && node ./run.js dev",
    "start": "cd scripts && node ./run.js start",
    "start:native": "cd native && npm run start",
    "test": "npm run test:domain && npm run test:web && npm run test:native",
    "test:domain": "cd domain && npm run test",
    "test:web": "cd web && npm run test",
    "test:native": "cd native && npm run test",
    "test:functional": "echo || exit 0"
  }
}
