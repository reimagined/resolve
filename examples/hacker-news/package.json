{
  "name": "hacker-news",
  "version": "0.12.3",
  "description": "A React & Redux & Resolve implementation of Hacker News",
  "private": true,
  "scripts": {
    "build": "resolve-scripts build",
    "start": "resolve-scripts start",
    "update": "resolve-scripts update",
    "dev": "resolve-scripts dev",
    "import": "babel-node ./import",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:functional": "resolve-scripts test:functional"
  },
  "dependencies": {
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "jsonwebtoken": "8.3.0",
    "nedb": "1.8.0",
    "passport-local": "1.0.0",
    "plur": "3.0.1",
    "progress": "2.0.0",
    "prop-types": "15.6.2",
    "query-string": "6.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-saga": "0.16.0",
    "resolve-bus-memory": "0.12.3",
    "resolve-es": "0.12.3",
    "resolve-redux": "0.12.3",
    "resolve-scripts": "0.12.3",
    "resolve-storage-lite": "0.12.3",
    "sanitizer": "0.1.3",
    "seamless-immutable": "7.1.3",
    "styled-components": "3.3.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/node": "7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "@babel/preset-stage-0": "7.0.0-beta.54",
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.4.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "23.4.1",
    "regenerator-runtime": "0.12.0",
    "sinon": "6.1.3",
    "testcafe": "0.20.4"
  },
  "jest": {
    "setupFiles": ["raf/polyfill", "<rootDir>/test/unit/jest-setup.js"],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist/",
      "coverage/",
      "resolve.build.config.js",
      "resolve.client.config.js",
      "resolve.server.config.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
