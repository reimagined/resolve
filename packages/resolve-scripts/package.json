{
  "name": "resolve-scripts",
  "version": "0.0.6",
  "description": "This package create server with resolve.",
  "main": "./dist/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "bin": {
    "resolve-scripts": "./dist/bin/resolve-scripts.js"
  },
  "scripts": {
    "prepublish": "babel --out-dir ./dist ./src",
    "build:client": "webpack --config ./_resolve/configs/webpack.client.config.js",
    "build:server": "webpack --config ./_resolve/configs/webpack.server.config.js",
    "build": "npm run cleanDist && npm run copyStatic && npm run build:client && npm run build:server",
    "copyStatic": "babel-node ./_resolve/scripts/copy.js",
    "cleanDist": "babel-node ./_resolve/scripts/clean.js",
    "dev:server": "webpack --config ./dist/scripts/dev.server.config.js",
    "dev:client": "webpack-dev-server --config ./dist/scripts/dev.client.config.js",
    "dev": "npm-run-all --parallel dev:client dev:server",
    "start": "cross-env NODE_ENV=production npm run build && node ./dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reimagined/resolve.git"
  },
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/resolve-redux#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "fs-extra": "^3.0.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.3.2",
    "start-server-webpack-plugin": "^2.2.0",
    "testcafe": "^0.15.0",
    "testcafe-browser-tools": "^1.2.1",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^8.0.2"
  }
}
