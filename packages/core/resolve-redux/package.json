{
  "name": "resolve-redux",
  "version": "0.23.0",
  "description": "Provides tools that you can use to connect a React/Redux frontend to reSolve.",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-redux#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "json-stable-stringify": "^1.0.1",
    "jwt-decode": "^2.2.0",
    "query-string": "^5.1.1",
    "unfetch": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "jest": "25.1.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-test-renderer": "16.13.0",
    "redux": "4.0.5",
    "sinon": "7.2.2",
    "react-redux": "^7.2.0",
    "redux-saga": "^1.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "redux": "*",
    "react-redux": "*",
    "redux-saga": "*"
  },
  "babelCompile": [
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
