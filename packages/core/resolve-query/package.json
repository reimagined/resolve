{
  "name": "resolve-query",
  "version": "0.26.2",
  "description": "Provides an interface used to create and querry read models and view models in a reSolve application.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-query#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=resolve-query",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "resolve-core": "0.26.2",
    "resolve-debug-levels": "0.26.2",
    "resolve-readmodel-base": "0.26.2"
  },
  "devDependencies": {
    "jest": "26.0.1"
  },
  "engines": {
    "node": ">=10.9"
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "es",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./es"
    },
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ],
  "sourceType": "ts"
