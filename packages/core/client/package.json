{
  "name": "@resolve-js/client",
  "version": "0.33.4",
  "description": "Basic reSolve client.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-api#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/client",
    "lint": "eslint --ignore-path=../../../.prettierignore './src/**/*.{ts,tsx}' './test/**/*.{ts,tsx}'",
    "test": "jest --config=../../../jest.config.js --verbose",
    "test:e2e": "jest --config=./jest.func-config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@resolve-js/core": "0.33.4",
    "global": "4.4.0",
    "isomorphic-fetch": "3.0.0",
    "node-fetch": "2.6.1",
    "query-string": "6.14.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "26.0.23",
    "@types/node-fetch": "2.5.8",
    "express": "4.17.1",
    "jest": "27.0.3",
    "ts-jest": "27.0.2"
  },
  "engines": {
    "node": ">=14.17"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    }
  ],
  "sourceType": "ts"
}
