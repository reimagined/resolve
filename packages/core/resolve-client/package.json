{
  "name": "resolve-client",
  "version": "0.24.3",
  "description": "Basic reSolve client.",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll",
    "test:functional": "jest --config=./jest.func-config.js --verbose",
    "lint": "eslint --ignore-path=../../../.prettierignore './src/**/*.{ts,tsx}' './test/**/*.{ts,tsx}'"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-api#readme",
  "dependencies": {
    "global": "^4.4.0",
    "node-fetch": "^2.6.0",
    "query-string": "^5.1.1",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "@types/express": "^4.17.1",
    "@types/node-fetch": "^2.5.5",
    "@types/jest": "^25.1.3",
    "jest": "26.0.1",
    "ts-jest": "26.0.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
