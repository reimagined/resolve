{
  "name": "resolve-react-hooks",
  "version": "0.24.23",
  "description": "reSolve client wrapper for using in React hooks projects.",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "echo || exit 0",
    "lint": "eslint --ignore-path=../../../.prettierignore './src/**/*.{ts,tsx}' './test/**/*.{ts,tsx}'"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-api#readme",
  "dependencies": {
    "resolve-client": "0.24.23",
    "resolve-core": "0.24.23"
  },
  "devDependencies": {
    "react": "16.13.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "jest": "26.0.1",
    "react-test-renderer": "16.13.0",
    "ts-jest": "26.0.0"
  },
  "peerDependencies": {
    "react": "^16.13.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "sourceType": "ts",
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    }
  ]
}
