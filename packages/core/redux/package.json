{
  "name": "@resolve-js/redux",
  "version": "0.28.1-alpha",
  "description": "Provides tools that you can use to connect a React/Redux frontend to reSolve.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/@resolve-js/redux#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@resolve-js/client": "0.28.1-alpha",
    "@resolve-js/core": "0.28.1-alpha",
    "@resolve-js/react-hooks": "0.28.1-alpha",
    "hoist-non-react-statics": "^3.3.2",
    "json-stable-stringify": "^1.0.1",
    "jwt-decode": "^2.2.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "lodash.set": "4.3.2",
    "lodash.unset": "4.5.2",
    "query-string": "^5.1.1",
    "unfetch": "^4.0.1",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.4.1",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.set": "4.3.6",
    "@types/lodash.unset": "4.5.6",
    "@types/query-string": "^5.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/redux-mock-store": "1.0.2",
    "@types/uuid": "^3.3.28",
    "jest": "26.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "redux-saga": "^1.0.0",
    "sinon": "7.2.2",
    "ts-jest": "26.5.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-saga": "*"
  },
  "engines": {
    "node": ">=10.9"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    }
  ],
  "sourceType": "ts"
}
