{
  "name": "@resolve-js/redux",
  "version": "0.34.1",
  "description": "Provides tools that you can use to connect a React/Redux frontend to reSolve.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/@resolve-js/redux#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/redux",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "@resolve-js/client": "0.34.1",
    "@resolve-js/core": "0.34.1",
    "@resolve-js/react-hooks": "0.34.1",
    "hoist-non-react-statics": "3.3.2",
    "json-stable-stringify": "1.0.1",
    "jwt-decode": "3.1.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "lodash.set": "4.3.2",
    "lodash.unset": "4.5.2",
    "query-string": "6.14.1",
    "unfetch": "4.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "5.1.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.set": "4.3.6",
    "@types/lodash.unset": "4.5.6",
    "@types/react-redux": "7.1.16",
    "@types/redux-mock-store": "1.0.2",
    "@types/uuid": "8.3.0",
    "jest": "27.5.1",
    "jest-mock": "27.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.0",
    "redux-mock-store": "1.5.4",
    "redux-saga": "1.1.3",
    "ts-jest": "27.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "0.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "engines": {
    "node": ">=14.17"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    }
  ],
  "sourceType": "ts"
}
