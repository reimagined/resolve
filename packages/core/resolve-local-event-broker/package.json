    "es",
    "ddd"
  ],
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-local-event-broker#readme",
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "sqlite": "^3.0.2",
    "tmp": "^0.1.0",
    "resolve-debug-levels": "0.24.11",
    "resolve-local-rpc": "0.24.11",
    "resolve-eventstore-base": "0.24.11",
    "resolve-readmodel-base": "0.24.11"
  },
  "devDependencies": {
    "sinon": "7.2.2",
    "jest": "26.0.1"
  },
  "babelCompile": [
    {
      "moduleType": "es",
      "moduleTarget": "server",
