{
  "name": "resolve-testing-tools",
  "version": "0.24.15",
  "description": "Tools used to test reSolve applications.",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-testing-tools#readme",
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "jest": "26.0.1",
    "sinon": "7.2.2",
    "resolve-core": "0.24.15",
    "resolve-query": "0.24.15",
    "resolve-command": "0.24.15",
    "resolve-readmodel-lite": "0.24.15",
    "resolve-eventstore-lite": "0.24.15"
  },
  "sourceType": "ts",
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "sourceType": "ts",
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
