{
  "name": "@resolve-js/scripts",
  "version": "0.28.1-alpha",
  "description": "Contains scripts used to initialize a reSolve server.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/@resolve-js/scripts#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll",
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/scripts"
  },
  "dependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/register": "7.12.13",
    "@babel/runtime": "7.13.10",
    "@purtuga/esm-webpack-plugin": "1.5.0",
    "@resolve-js/debug-levels": "0.28.1-alpha",
    "@resolve-js/runtime": "0.28.1-alpha",
    "address": "1.1.2",
    "ajv": "7.2.1",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chalk": "4.1.0",
    "deepmerge": "4.2.2",
    "fs-extra": "9.1.0",
    "glob": "7.1.6",
    "isomorphic-fetch": "3.0.0",
    "loader-utils": "2.0.0",
    "open": "8.0.2",
    "respawn": "2.6.0",
    "route-trie": "3.0.0",
    "socket.io-client": "4.0.0",
    "testcafe": "1.12.0",
    "testcafe-browser-tools": "2.0.15",
    "uuid": "8.3.2",
    "webpack": "4.42.0",
    "webpack-node-externals": "2.5.2"
  },
  "devDependencies": {
    "jest": "26.6.3",
    "lodash.escaperegexp": "4.1.2",
    "sinon": "9.2.4"
  },
  "engines": {
    "node": ">=10.9"
  },
  "babelCompile": [
    {
      "moduleType": "es",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./es"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
