{
  "name": "@resolve-js/testing-tools",
  "version": "0.29.0",
  "description": "Tools used to test reSolve applications.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-testing-tools#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/testing-tools",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@resolve-js/core": "0.29.0",
    "@resolve-js/runtime": "0.29.0",
    "@resolve-js/readmodel-lite": "0.29.0",
    "colors": "^1.4.0",
    "diff": "^4.0.2",
    "lodash.isequal": "4.5.0",
    "lodash.partial": "4.2.1"
  },
  "devDependencies": {
    "@types/diff": "4.0.2",
    "@types/lodash.isequal": "4.5.5",
    "@types/lodash.partial": "4.2.6",
    "jest": "26.6.3",
    "sinon": "7.2.2"
  },
  "engines": {
    "node": ">=12.18"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    },
    {
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    }
  ],
  "sourceType": "ts"
}
