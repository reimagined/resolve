{
  "name": "resolve-readmodel-base",
  "version": "0.14.4",
  "description": "Base package for readModel adapters",
  "engines": {
    "node": ">=8.10"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "cross-env MODULE_TYPE=es MODULE_TARGET=server babel --config-file=../../../../babel.config.js --source-maps --out-dir ./es ./src",
    "build:commonjs": "cross-env MODULE_TYPE=cjs MODULE_TARGET=server babel --config-file=../../../../babel.config.js --source-maps --out-dir ./lib ./src",
    "test": "jest --config=../../../../jest.config.js --verbose",
    "testw": "jest --config=../../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/readmodel-adapters/resolve-readmodel-base#readme",
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "jest": "23.5.0",
    "sinon": "6.2.0"
  }
}