  },
  "scripts": {
    "test": "jest --config=../../../../jest.config.js --verbose",
    "testw": "jest --config=../../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/storage-adapters/resolve-storage-postgresql#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "resolve-storage-base": "0.22.3",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "jest": "24.1.0"
  },
  "peerDependencies": {
    "aws-sdk": "*"
  },
  "babelCompile": [
    {
      "moduleType": "es",
