{
  "name": "resolve-storage-base",
  "version": "0.14.3",
  "description": "This package contains the basic components for storage adapters",
  "engines": {
    "node": ">=8.10"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "cross-env MODULE_TYPE=es MODULE_TARGET=server babel --config-file=../../../../babel.config.js --source-maps --out-dir ./es ./src",
    "build:commonjs": "cross-env MODULE_TYPE=cjs MODULE_TARGET=server babel --config-file=../../../../babel.config.js --source-maps --out-dir ./lib ./src",
    "test": "echo || exit 0",
    "testw": "echo || exit 0"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/storage-adapters/resolve-storage-base#readme"
}