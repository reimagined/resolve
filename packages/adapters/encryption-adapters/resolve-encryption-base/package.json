{
  "name": "resolve-encryption-base",
  "version": "0.23.2",
  "description": "reSolve cloud encryption adapters base module",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest",
    "testw": "echo || exit 0",
    "lint": "eslint --ignore-path=../../../.prettierignore './src/**/*.{ts,tsx}' './test/**/*.{ts,tsx}'"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-api#readme",
  "dependencies": {
    "cryptr": "^6.0.2",
    "resolve-cloud-common": "^0.0.15",
    "stream": "^0.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cryptr": "^4.0.0",
    "@types/node": "^13.7.0",
    "@types/pg": "^7.14.3",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/uuid": "^7.0.2",
    "jest": "25.1.0",
    "ts-jest": "^25.2.1"
  },
  "peerDependencies": {},
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
