{
  "name": "resolve-subscribe-mqtt",
  "version": "0.16.1",
  "description": "This package serves as an adapter for resolve-redux subscribe to events.",
  "engines": {
    "node": ">=8.10"
  },
  "main": "./lib/server/index.js",
  "browser": "./lib/client/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../../jest.config.js --verbose",
    "testw": "jest --config=../../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "dependencies": {
    "mqtt": "^2.18.8",
    "ws": "^5.2.1",
    "websocket-stream": "^5.1.2",
    "mqtt-connection": "^4.0.0"
  },
  "devDependencies": {
    "jest": "23.6.0"
  },
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/subscribe-adapters/resolve-subscribe-mqtt#readme",
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src/client",
      "outDir": "./lib/client"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "server",
      "inputDir": "./src/server",
      "outDir": "./lib/server"
    }
  ]
}