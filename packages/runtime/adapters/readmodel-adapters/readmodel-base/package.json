{
  "name": "@resolve-js/readmodel-base",
  "version": "0.34.3",
  "description": "A base package for reSolve read model adapters",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/readmodel-adapters/@resolve-js/readmodel-base#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/readmodel-base",
    "test": "jest --config=../../../../../jest.config.js --verbose",
    "testw": "jest --config=../../../../../jest.config.js --watchAll"
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "@resolve-js/core": "0.34.3",
    "@resolve-js/eventstore-base": "0.34.3",
    "path-toolkit": "1.2.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "jest": "27.5.1"
  },
  "engines": {
    "node": ">=14.17"
  },
  "babelCompile": [
    {
      "moduleType": "es",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./es"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "server",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ],
  "files": [
    "/lib/**/*.js",
    "types",
    "configs",
    "es",
    "typings",
    "index.d.ts",
    "bin"
  ],
  "sourceType": "ts"
}
