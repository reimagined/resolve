{
  "name": "resolve-module-comments",
  "version": "0.20.28",
  "description": "A reSolve module that allows an application's users to post comments.",
  "engines": {
    "node": ">=8.10"
  },
  "main": "./lib/common/index.js",
  "browser": "./lib/client/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/modules/resolve-module-comments#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "resolve-readmodel-lite": "0.20.28"
  },
  "devDependencies": {
    "jest": "24.1.0",
    "sinon": "7.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*",
    "resolve-redux": "*"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src/common",
      "outDir": "./lib/common"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src/client",
      "outDir": "./lib/client"
    }
  ]
}
