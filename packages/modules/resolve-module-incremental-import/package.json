{
  "name": "resolve-module-incremental-import",
  "version": "0.24.10",
  "description": "A reSolve module that adds incremental import",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/modules/resolve-module-incremental-import#readme",
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "iconv-lite": "0.6.2"
  },
  "devDependencies": {
    "jest": "26.0.1"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
