{
  "name": "@resolve-js/module-comments",
  "version": "0.31.7",
  "description": "A reSolve module that allows an application's users to post comments.",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/modules/@resolve-js/module-comments#readme",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "license": "MIT",
  "author": "reimagined team",
  "main": "./lib/common/index.js",
  "browser": "./lib/client/index.js",
  "scripts": {
    "build": "yarn workspace @internal/babel-compile start --name=@resolve-js/module-comments",
    "test": "jest --config=../../../jest.config.js --verbose",
    "testw": "jest --config=../../../jest.config.js --watchAll"
  },
  "dependencies": {
    "@babel/runtime": "7.14.0"
  },
  "devDependencies": {
    "@resolve-js/redux": "0.31.7",
    "jest": "27.0.3",
    "react": "17.0.2",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "sinon": "9.2.4"
  },
  "peerDependencies": {
    "@resolve-js/redux": "0.31.7",
    "react": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "engines": {
    "node": ">=12.18"
  },
  "babelCompile": [
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src/common",
      "outDir": "./lib/common"
    },
    {
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src/client",
      "outDir": "./lib/client"
    }
  ]
}
