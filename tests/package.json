{
  "name": "tests",
  "version": "0.33.9",
  "private": true,
  "description": "API tests for resolve",
  "scripts": {
    "test": "echo || exit 0",
    "test:integration": "jest --config=./jest.config.js",
    "test:postgres": "yarn test:eventstore-postgres && yarn test:read-model-postgres",
    "test:read-model": "jest --config=./jest.config.js --testMatch=**/read-model-store-*/*.test.[jt]s --runInBand",
    "test:read-model-mysql": "env TEST_MYSQL=true jest --config=./jest.config.js --testMatch=**/read-model-store-*/*.test.[jt]s",
    "test:read-model-postgres": "env TEST_POSTGRES=true jest --config=./jest.config.js --testMatch=**/read-model-store-*/*.test.[jt]s --runInBand",
    "test:read-model-builder-postgres": "env TEST_POSTGRES=true jest --config=./jest.config.js --testMatch=**/read-model-builder-postgres/*.test.[jt]s --runInBand",
    "test:eventstore-order-events": "jest --config=./jest.config.js --testMatch=**/eventstore-order-events/*.test.[jt]s",
    "test:eventstore": "jest --config=./jest.config.js --testMatch=**/eventstore-*/*.test.[jt]s",
    "test:eventstore-postgres": "env TEST_POSTGRES=true jest --config=./jest.config.js --testMatch=**/eventstore-*/*.test.[jt]s --runInBand",
    "test:import-export": "jest --config=./jest.config.js --testMatch=**/import-export-sample/*.test.ts",
    "test:secrets": "jest --config=./jest.config.js --testMatch=**/eventstore-secrets/*.test.ts",
    "test:filter-events": "jest --config=./jest.config.js --testMatch=**/eventstore-filter-events/*.test.ts",
    "test:incremental-import": "jest --config=./jest.config.js --testMatch=**/incremental-import/*.test.[jt]s",
    "test:replication": "jest --config=./jest.config.js --testMatch=**/eventstore-replication/*.test.ts",
    "test:freeze": "jest --config=./jest.config.js --testMatch=**/eventstore-freeze-unfreeze/*.test.ts",
    "test:save-load": "jest --config=./jest.config.js --testMatch=**/eventstore-save-load/*.test.ts",
    "test:parallel-save": "jest --config=./jest.config.js --testMatch=**/eventstore-parallel-save/*.test.ts",
    "test:init-drop": "jest --config=./jest.config.js --testMatch=**/eventstore-init-drop/*.test.[jt]s",
    "test:connect-dispose": "jest --config=./jest.config.js --testMatch=**/eventstore-connect-dispose/*.test.[jt]s",
    "test:timeout-error": "jest --config=./jest.config.js --testMatch=**/eventstore-timeout/*.test.[jt]s",
    "test:node-postgres": "jest --config=./jest.config.js --testMatch=**/node-postgres/*.test.[jt]s"
  },
  "devDependencies": {
    "@babel/runtime": "7.14.0",
    "@resolve-js/core": "0.33.9",
    "@resolve-js/debug-levels": "0.33.9",
    "@resolve-js/eventstore-base": "0.33.9",
    "@resolve-js/eventstore-lite": "0.33.9",
    "@resolve-js/eventstore-postgresql": "0.33.9",
    "@resolve-js/monitoring-base": "0.33.9",
    "@resolve-js/readmodel-lite": "0.33.9",
    "@resolve-js/readmodel-mysql": "0.33.9",
    "@resolve-js/readmodel-postgresql": "0.33.9",
    "@resolve-js/scripts": "0.33.9",
    "@resolve-js/testing-tools": "0.33.9",
    "jest": "27.0.3",
    "jsonwebtoken": "8.5.1",
    "pg": "8.5.1",
    "pg-cursor": "2.7.1"
  }
}
